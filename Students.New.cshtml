@model StudentRegistrationApplication.Models.Student
@{
    /**/

    ViewBag.Title = "New";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>New Student</h2>
@using (Html.BeginForm("Create", "Students"))
{
    <div class="form-group">
        @Html.LabelFor(m => m.Name)
        @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(m => m.IndexNo)
        @Html.TextBoxFor(m => m.IndexNo, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(m => m.RegisterDate)
        @Html.TextBoxFor(m => m.RegisterDate, new { @class = "form-control" })
    </div>

}

